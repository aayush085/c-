#include <iostream>
using namespace std;

int main() {
    int aa;
 int a[5]={4,1,5,8,6};
     for(int i=0;i<=5;i++){
         for(int j=i+1;j<5;j++){
            //  a[i]=a[j];
             if(a[i]>a[j]){
                 aa=a[i];
                 a[i]=a[j];
                 a[j]=aa;
                 
             }}}
       for(int i=0;i<=5;i++) {
           cout<<a[i]<<",";
       } 
     
     
}
